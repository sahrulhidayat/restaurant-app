{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:codeceptjs/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "no-undef": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "eqeqeq": 0,
        "no-return-assign": 0,
        "no-new": 0,
        "class-methods-use-this": 0
    }
}
